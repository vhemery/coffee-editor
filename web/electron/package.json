{
  "private": true,
  "name": "electron",
  "productName": "DISCO",
  "description": "Electron app for DISCO",
  "version": "1.2.0",
  "author": {
    "name": "Vincent HEMERY",
    "email": "vincent.hemery@csgroup.eu",
    "url": "https://accounts.eclipse.org/users/vhemery"
  },
