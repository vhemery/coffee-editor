  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "main": "DISCO-main.js",
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "DISCO"
      }
    }
  },
  "dependencies": {
    "@theia/callhierarchy": "^1.2.0",
    "@theia/console": "^1.2.0",
    "@theia/core": "^1.2.0",
    "@theia/debug": "^1.2.0",
    "@theia/editor": "^1.2.0",
    "@theia/electron": "^1.2.0",
    "@theia/file-search": "^1.2.0",
    "@theia/filesystem": "^1.2.0",
    "@theia/json": "^1.2.0",
    "@theia/keymaps": "^1.2.0",
    "@theia/languages": "^1.2.0",
    "@theia/markers": "^1.2.0",
    "@theia/messages": "^1.2.0",
    "@theia/mini-browser": "^1.2.0",
    "@theia/monaco": "^1.2.0",
    "@theia/navigator": "^1.2.0",
    "@theia/outline-view": "^1.2.0",
    "@theia/output": "^1.2.0",
    "@theia/plugin-dev": "^1.2.0",
    "@theia/plugin-ext": "^1.2.0",
    "@theia/plugin-ext-vscode": "^1.2.0",
    "@theia/preferences": "^1.2.0",
    "@theia/process": "^1.2.0",
    "@theia/search-in-workspace": "^1.2.0",
    "@theia/terminal": "^1.2.0",
    "@theia/typehierarchy": "^1.2.0",
    "@theia/userstorage": "^1.2.0",
    "@theia/variable-resolver": "^1.2.0",
    "@theia/workspace": "^1.2.0",
    "coffee-editor-extension": "0.7.0",
    "coffee-java-extension": "0.7.0",
    "coffee-server": "0.7.0",
    "coffee-welcome-page": "0.7.0",
    "fs-extra": "^9.0.1",
    "sharp": "^0.25.3",
    "vscode-jsonrpc": "^5.0.1"
  },
  "scripts": {
    "lint": "theiaext lint",
    "clean": "theia clean",
    "build": "theiaext compile && yarn bundle",
    "rebuild:electron": "theia rebuild:electron",
    "prepare": "theia build --mode development",
    "bundle": "theia build --mode development",
    "watch": "concurrently -n compile,bundle \"theiaext watch --preserveWatchOutput\" \"theia build --watch --mode development\"",
    "start": "yarn rebuild:electron && theia start --WORKFLOW_NOTATION_LSP=5008 --WF_LSP=5017 --hostname 0.0.0.0 --loglevel=debug --root-dir=../../backend/examples/ --plugins=local-dir:./plugins",
    "start:debug": "yarn start --log-level=debug",
    "test": "electron-mocha --timeout 60000 \"./lib/test/**/*.espec.js\"",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@theia/cli": "^1.2.0",
    "electron": "^4.2.11",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.11.0"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-builtin-configuration-editing": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/configuration-editing-1.39.1-prel.vsix",
